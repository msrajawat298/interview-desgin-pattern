{
  "name" : "skaleet/interview",
  "type" : "project",
  "autoload" : {
    "psr-4" : {
      "Skaleet\\Interview\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Skaleet\\Interview\\Tests\\" : "tests/"
    }
  },
  "require" : {
    "php" : "~8.0",
    "php-di/php-di" : "^7.0",
    "symfony/console" : "^6.0"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^9.5"
  },
  "scripts": {
    "test": "phpunit --coverage-html coverage",
    "coverage": "phpdbg -qrr ./vendor/bin/phpunit --coverage-text",
    "testdox": "./vendor/bin/phpunit --testdox"
  }
}
